<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sun.modules.bean.dao.IUserDAO">

    <insert id="insertUserInfo" parameterType="list">
        INSERT into user(`name`,`age`,`sex`,`add_time`,`modification_time`)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.name},#{item.age},#{item.sex},unix_timestamp(),unix_timestamp())
        </foreach>
    </insert>
</mapper>