<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sun.moudles.bean.domain.VideoDO">

    <insert id="insertVideoInfo" parameterType="list">
        INSERT into video(v_name,v_time,v_actors,v_type,v_url,v_picture_path,v_detail,add_time,motification_time)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.videoName},#{item.videoTime},#{item.videoActors},#{item.videoType},
            #{item.videoUrl},#{item.videoPicturePath},#{item.videoDetail},unix_timestamp(),unix_timestamp())
        </foreach>
    </insert>
</mapper>